{
  "author": "Superagent",
  "chrome_url_overrides": {
     "newtab": "index.html"
  },
  "background": {
    "scripts": [ "background.js" ]
  },
  "description": "A chrome extension that helps you be more productive. Fewer tabs = happy CPU pet. More tabs = sad CPU pet.",
  "homepage_url": "https://besuperagent.com",
  "icons": {
     "128": "ninja.png",
     "16": "ninja.png"
  },
  "manifest_version": 2,
  "name": "Closer",
  "permissions": [ "storage", "tabs", "activeTab","*://*.giphy.com/*" ],
  "short_name": "Closer",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "content_security_policy": "script-src 'self' 'sha256-dnYhfXrSM6/f1zANcM/hHI/Ue5PwgGSkVtEEjGQjCR0='; object-src 'self'"
}
